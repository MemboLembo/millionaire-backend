{
  "port": 3000,
  "mongoose": {
    "uri": "mongodb://localhost:27017/Millionaire",
    "options": {
      "useNewUrlParser": true,
      "useUnifiedTopology": true
    }
  },
  "log4js": {
    "appenders": {
      "app": {
        "type": "stdout"
      }
    },
    "categories": {
      "default": {
        "appenders": ["app"],
        "level": "debug"
      }
    }
  }
}
